{
  "name": "seizu",
  "version": "2.0.0",
  "description": "Contract-based state transition library for TypeScript",
  "keywords": [
    "contract",
    "state-transition",
    "ddd",
    "typescript",
    "pbt"
  ],
  "author": "seizu Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fyuuki0jp/seizu.git",
    "directory": "packages/seizu"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./testing": {
      "import": {
        "types": "./dist/testing.d.ts",
        "default": "./dist/testing.js"
      },
      "require": {
        "types": "./dist/testing.d.cts",
        "default": "./dist/testing.cjs"
      }
    },
    "./verify": {
      "import": {
        "types": "./dist/verify/index.d.ts",
        "default": "./dist/verify/index.js"
      },
      "require": {
        "types": "./dist/verify/index.d.cts",
        "default": "./dist/verify/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run"
  },
  "peerDependencies": {
    "fast-check": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "fast-check": {
      "optional": true
    }
  },
  "devDependencies": {
    "fast-check": "^3.23.2"
  }
}
