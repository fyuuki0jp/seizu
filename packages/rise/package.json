{
  "name": "rise",
  "version": "2.0.0",
  "description": "Contract-based state transition library for TypeScript",
  "keywords": [
    "contract",
    "state-transition",
    "ddd",
    "typescript",
    "pbt"
  ],
  "author": "RISE Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rise-es/rise.git",
    "directory": "packages/rise"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./testing": {
      "import": {
        "types": "./dist/testing.d.ts",
        "default": "./dist/testing.js"
      },
      "require": {
        "types": "./dist/testing.d.cts",
        "default": "./dist/testing.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run"
  },
  "dependencies": {}
}
